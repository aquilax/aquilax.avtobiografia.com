{
	"@context": ["https://www.w3.org/ns/activitystreams", "https://w3id.org/security/v1", {
		"manuallyApprovesFollowers": "as:manuallyApprovesFollowers",
		"toot": "http://joinmastodon.org/ns#",
		"featured": {
			"@id": "toot:featured",
			"@type": "@id"
		},
		"featuredTags": {
			"@id": "toot:featuredTags",
			"@type": "@id"
		},
		"alsoKnownAs": {
			"@id": "as:alsoKnownAs",
			"@type": "@id"
		},
		"movedTo": {
			"@id": "as:movedTo",
			"@type": "@id"
		},
		"schema": "http://schema.org#",
		"PropertyValue": "schema:PropertyValue",
		"value": "schema:value",
		"IdentityProof": "toot:IdentityProof",
		"discoverable": "toot:discoverable",
		"Device": "toot:Device",
		"Ed25519Signature": "toot:Ed25519Signature",
		"Ed25519Key": "toot:Ed25519Key",
		"Curve25519Key": "toot:Curve25519Key",
		"EncryptedMessage": "toot:EncryptedMessage",
		"publicKeyBase64": "toot:publicKeyBase64",
		"deviceId": "toot:deviceId",
		"claim": {
			"@type": "@id",
			"@id": "toot:claim"
		},
		"fingerprintKey": {
			"@type": "@id",
			"@id": "toot:fingerprintKey"
		},
		"identityKey": {
			"@type": "@id",
			"@id": "toot:identityKey"
		},
		"devices": {
			"@type": "@id",
			"@id": "toot:devices"
		},
		"messageFranking": "toot:messageFranking",
		"messageType": "toot:messageType",
		"cipherText": "toot:cipherText",
		"suspended": "toot:suspended",
		"focalPoint": {
			"@container": "@list",
			"@id": "toot:focalPoint"
		}
	}],
	"id": "{{ .Site.BaseURL }}users/{{ .Title }}",
	"type": "Person",
	"following": "{{ .Site.BaseURL }}users/{{ .Title }}/following",
	"followers": "{{ .Site.BaseURL }}users/{{ .Title }}/followers",
	"inbox": "{{ .Site.BaseURL }}users/{{ .Title }}/inbox",
	"outbox": "{{ .Site.BaseURL }}users/{{ .Title }}/outbox",
	"featured": "{{ .Site.BaseURL }}users/{{ .Title }}/collections/featured",
	"featuredTags": "{{ .Site.BaseURL }}users/{{ .Title }}/collections/tags",
	"preferredUsername": "{{ .Title }}",
	"name": "{{ .Title }}",
	"summary": {{ .Params.Summary | jsonify }},
	"url": "{{ .Site.BaseURL }}@{{ .Title }}",
	"manuallyApprovesFollowers": false,
	"discoverable": true,
	"devices": "{{ .Site.BaseURL }}users/{{ .Title }}/collections/devices",
	"publicKey": {
		"id": "{{ .Site.BaseURL }}users/{{ .Title }}#main-key",
		"owner": "{{ .Site.BaseURL }}users/{{ .Title }}",
		"publicKeyPem": {{ .Params.PublicKeyPem | jsonify }}
	},
	"tag": [],
	"attachment": [{
		"type": "PropertyValue",
		"name": "Bio",
		"value": {{ .Params.Bio | jsonify }}
	}],
	"endpoints": {
		"sharedInbox": "{{ .Site.BaseURL }}inbox"
	},
	"icon": {
		"type": "Image",
		"mediaType": "image/jpeg",
		"url": "{{ .Params.Image }}"
	},
	"image": {
		"type": "Image",
		"mediaType": "image/jpeg",
		"url": "{{ .Params.Image }}"
	}
}